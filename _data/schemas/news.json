{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$ref": "#/definitions/News",
    "definitions": {
        "News": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "news_list": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/NewsList"
                    }
                }
            },
            "required": [
                "news_list"
            ],
            "title": "News"
        },
        "NewsList": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "title": {
                    "type": "string"
                },
                "source": {
                    "type": "string"
                },
                "date": {
                    "type": "string",
                    "format": "date"
                },
                "media_url": {
                    "type": "string"
                },
                "media_type": {
                    "$ref": "#/definitions/MediaType"
                },
                "sub_source": {
                    "type": "string"
                }
            },
            "required": [
                "date",
                "media_type",
                "source",
                "title"
            ],
            "title": "NewsList"
        },
        "MediaType": {
            "type": "string",
            "enum": [
                "print",
                "audio",
                "video"
            ],
            "title": "MediaType"
        }
    }
}
